<?xml version="1.0" encoding="utf-8"?>
<TestSuite Id="TS_001" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="TestSuiteTemplate.xsd">
	<Name>针对 NextDate 函数的测试套件</Name>
	<Target>TestFunctions.TriangleTest()</Target>
	<TestCases>
		<TestCase Id="TC_0001">
			<Name>针对 TriangleTest 的路径覆盖测试用例</Name>
			<Assertions>
				<Assertion Id="TA_001">
					<InputValues>
						<InputValue Index="0" DataType="double">3.0</InputValue>
						<InputValue Index="1" DataType="double">4.5</InputValue>
					</InputValues>
					<ExpectedOutput>#ab</ExpectedOutput>
					<FactOutput>#ab</FactOutput>
					<Result>success</Result>
				</Assertion>
				<Assertion Id="TA_002">
					<InputValues>
						<InputValue Index="0" DataType="double">4.0</InputValue>
						<InputValue Index="1" DataType="double">3.5</InputValue>
					</InputValues>
					<ExpectedOutput>#</ExpectedOutput>
					<FactOutput>#ab</FactOutput>
					<Result>fail</Result>
				</Assertion>
			</Assertions>
		</TestCase>
		<TestCase Id="TC_0002">
			<Name>针对 TriangleTest 的边界值测试用例</Name>
			<Assertions>
				<Assertion Id="TA_003">
					<InputValues>
						<InputValue Index="0" DataType="dboule">0.0</InputValue>
						<InputValue Index="1" DataType="dboule">0.0</InputValue>
					</InputValues>
					<ExpectedOutput>边长不能为 0</ExpectedOutput>
					<FactOutput>边长不能为 0</FactOutput>
					<Result>success</Result>
				</Assertion>
				<Assertion Id="TA_004">
					<InputValues>
						<InputValue Index="0" DataType="dboule">10.0</InputValue>
						<InputValue Index="1" DataType="dboule">10.0</InputValue>
					</InputValues>
					<ExpectedOutput>边长必须小于 10</ExpectedOutput>
					<FactOutput>#ab</FactOutput>
					<Result>fail</Result>
				</Assertion>
			</Assertions>
		</TestCase>
	</TestCases>
</TestSuite>
